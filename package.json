    "@material-ui/core": "^4.9.5",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/pickers": "^3.2.10",
    "axios": "^0.19.1",
    "express": "^4.17.1",
    "moment": "^2.29.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-helmet-async": "^1.0.4",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "react-sortable-hoc": "^1.11.0",
    "rxjs": "^6.5.4",
    "rxjs-compat": "^6.5.4"
